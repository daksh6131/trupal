{
  "name": "creatr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "preview": "next build && next start",
    "start": "WATCHPACK_POLLING=true NEXT_TELEMETRY_DISABLED=1 next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "test": "jest",
    "test:types": "tsc --noEmit --pretty",
    "test:all": "pnpm run test && pnpm run test:types",
    "db:generate": "npx drizzle-kit generate && npx drizzle-kit migrate && git add . && git commit -m 'chore: update migration'",
    "clean-install": "rm -rf node_modules && rm -rf .bun && bun install --no-cache --no-symlinks"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.1",
    "drizzle-orm": "0.41.0-29155cc",
    "error-stack-parser": "^2.1.4",
    "geist": "^1.3.1",
    "lucide-react": "^0.503.0",
    "next": "^15.3.1",
    "next-transpile-modules": "^10.0.1",
    "node-libs-browser": "^2.2.1",
    "postgres": "3.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-loading-skeleton": "^3.5.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^3.2.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "22.15.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "source-map": "^0.7.4",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.8.3"
  }
}
