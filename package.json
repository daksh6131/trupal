{
  "name": "creatr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "preview": "next build && next start",
    "start": "WATCHPACK_POLLING=true NEXT_TELEMETRY_DISABLED=1 next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "test": "jest",
    "test:types": "tsc --noEmit --pretty",
    "test:all": "pnpm run test && pnpm run test:types",
    "db:generate": "npx drizzle-kit generate && npx drizzle-kit migrate && git add . && git commit -m 'chore: update migration'",
    "clean-install": "rm -rf node_modules && rm -rf .bun && bun install --no-cache --no-symlinks"
  },
  "dependencies": {
    "crypto-browserify": "^3.12.1",
    "drizzle-orm": "0.41.0-29155cc",
    "error-stack-parser": "^2.1.4",
    "next-transpile-modules": "^10.0.1",
    "node-libs-browser": "^2.2.1",
    "react-loading-skeleton": "^3.5.0",
    "stream-browserify": "^3.0.0"
  }
}